#include <stdio.h>

int main(void)
{
    char c = 'A';
    printf("%c\n",c);
    char str[256];
    scanf("%s", str, sizeof(str));
    printf("%s\n", str);
    return 0;
}

//예제
#include <stdio.h>

int main(void)
{
    char name[256];
    printf("이름이 뭐엇입니까?");
    scanf("%s", name);

    int age;
    printf("몇살입니까?");
    scanf("%d", &age);

    float weight;
    printf("몸무게는 얼마입니까?");
    scanf("%f", &weight);

    double height;
    printf("키는 얼마입니까?");
    scanf("%lf", &height);

    char what[256];
    printf("무슨 잘못을 했습니까?");
    scanf("%s", what);

    printf("\n\n--- 범죄자 정보---\n\n");
    printf("이름 : %s\n", name);
    printf("나이 : %d\n", age);
    printf("몸무게: %.2f\n", weight);
    printf("키 : %.2lf\n", height);
    printf("죄명 : %s\n", what);

    return 0;
}
