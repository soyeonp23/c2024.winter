#include <stdio.h>

int main(){
  printf("hello world|n");
}

//정수형 변수 예제
#include <stdio.h>
int main(void)
{
    int age = 12;
    printf("%d\n", age);
    age = 13;
    printf("%d\n", age);
    return 0;
}

//실수형변수에 대한 예
#include <stdio.h>
int main(void)
{
    float f = 46.5f;
    printf("%f\n",f);
    return 0;
    double d = 4.428;
    printf("%f\n", d);
    return 0;
}
//소수점 수 지정
#include <stdio.h>
int main(void)
{
    float f = 46.5f;
    printf("%.2f\n",f);
    return 0;
    double d = 4.428;
    printf("%.2f\n", d);
    return 0;
}

//상수
#include <stdio.h>
int main(void)
{
  const int YEAR = 2000;
  printf("태어난 년도 : %d\n", YEAR);
  //YEAR = 2001;
  return 0;
}
//printf
#include <stdio.h>
int main(void)
{
  int add = 3 + 7; //10
  printf("3+7= %d\n", add);
  printf("%d+%d = %d\n", 3, 7, 3+7);
  return 0;
}

//scanf
#include <stdio.h>
int main(void)
{
  int input;
  printf("값을 입력하세요 : ");
  scanf("%d", &input); //scanf 말고 scanf_s 필요할수도
  printf("입력값 : %d\n", input);
  
  return 0;
}
#include <stdio.h>
int main(void)
{
  int one, two, three;
  printf("세개의 정수를 입력하세요 :");
  scanf("%d, %d, %d", &one, &two, &three);
  printf("첫번째값: %d\n", one);
  printf("첫번째값: %d\n", two);
  printf("첫번째값: %d\n", three);
  return 0;
}
